{
  "expo": {
    "name": "MiskiFast",
    "slug": "miskifast",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "backgroundColor": "#121223",
    "assets": ["./assets/fonts/"],
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#574B36"
    },
    "ios": {
      "icon": "./assets/images/icon.png",
      "supportsTablet": true,
      "statusBarStyle": "light",
      "statusBarBackgroundColor": "#00B894",
      "bundleIdentifier": "com.sode.miskifast",
      "splash": {
        "image": "./assets/images/icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#574B36",
        "dark": {
          "image": "./assets/images/icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#2D2A24"
        }
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "This app needs access to camera to take photos and videos for support tickets.",
        "NSPhotoLibraryUsageDescription": "This app needs access to photo library to select images and videos for support tickets.",
        "NSMicrophoneUsageDescription": "This app needs access to microphone for support features.",
        "NSLocationWhenInUseUsageDescription": "This app may use location for device diagnostics.",
        "UIBackgroundModes": ["remote-notification"]
      }
    },
    "android": {
      "icon": "./assets/images/icon.png",
      "softwareKeyboardLayoutMode": "pan",
      "windowSoftInputMode": "adjustPan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#574B36",
        "monochromeImage": "./assets/images/icon.png"
      },
      "jsEngine": "hermes",
      "enableProguardInReleaseBuilds": false,
      "enableSeparateBuildPerCPUArchitecture": false,
      "splash": {
        "image": "./assets/images/icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#574B36",
        "dark": {
          "image": "./assets/images/icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#2D2A24"
        }
      },
      "statusBar": {
        "backgroundColor": "#574B36",
        "barStyle": "light-content"
      },
      "navigationBar": {
        "backgroundColor": "#966e28ff",
        "barStyle": "light-content"
      },

      "package": "com.sode.miskifast",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.ACCESS_WIFI_STATE",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.WAKE_LOCK",
        "android.permission.VIBRATE",
        "android.permission.USE_FULL_SCREEN_INTENT",
        "com.google.android.c2dm.permission.RECEIVE",
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION"
      ],
      "usesCleartextTraffic": true,
      "networkSecurityConfig": {
        "cleartextTrafficPermitted": true
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png",
      "backgroundColor": "#00B894"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-system-ui",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#D4AF37",
          "defaultChannel": "default",
          "sounds": ["./assets/sounds/message.mp3"]
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useModularHeaders": true,
            "useFrameworks": "static"
          },
          "android": {
            "usesCleartextTraffic": true,
            "networkSecurityConfig": {
              "cleartextTrafficPermitted": true
            }
          }
        }
      ],
      "expo-secure-store",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permitir que la app acceda a tu ubicaci√≥n."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4a759cf2-9dcd-4341-b447-dda10d7275e0"
      }
    }
  }
}
